n        self.pos_encoding_layer = torch.nn.Embedding(max_context_len, model_dim)\n\n        self.encoder_layer_stack = torch.nn.ModuleList()\n\n        for _ in range(num_encoder_layers):\n            self.encoder_layer_stack.append(\n                EncoderLayer(model_dim,num_attn_heads)\n            )\n\n        self.classification_head = torch.nn.Linear(model_dim, num_classes)\n\n    def forward(self,X):\n\n        token_embedding = self.embedding_layer(X)\n\n        position_ids = torch.arange(\n            0, self.model_context_len, device=X.device\n        ).unsqueeze(0)\n\n        pos_encoding = self.pos_encoding_layer(position_ids)\n\n        input_embedding = token_embedding + pos_encoding\n\n        for layer in self.encoder_layer_stack:\n            input_embedding = layer(input_embedding)\n\n        cls_embedding = input_embedding[:,0]\n\n        logits = self.classification_head(cls_embedding)\n\n        return logits","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-02-03T15:11:59.940334Z","iopub.execute_input":"2026-02-03T15:11:59.940568Z","iopub.status.idle":"2026-02-03T15:11:59.954254Z","shell.execute_reply.started":"2026-02-03T15:11:59.940532Z","shell.execute_reply":"2026-02-03T15:11:59.953532Z"}},"outputs":[],"execution_count":30},{"cell_type":"code","source":"model = BERT(\n    vocab_size=30522,\n    max_context_len=512,\n    model_dim=768,\n    num_encoder_layers=12,\n    num_attn_heads=12,\n    num_classes=num_classes,\n    model_context_len=512\n)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-02-03T15:11:59.955110Z","iopub.execute_input":"2026-02-03T15:11:59.955411Z","iopub.status.idle":"2026-02-03T15:12:00.857420Z","shell.execute_reply.started":"2026-02-03T15:11:59.955349Z","shell.execute_reply":"2026-02-03T15:12:00.856702Z"}},"outputs":[],"execution_count":31},{"cell_type":"code","source":"bert_model = BertForSequenceClassification.from_pretrained(\"bert-base-uncased\",num_labels = num_classes)","metada